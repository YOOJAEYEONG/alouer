<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="mybatis.AndroidImpl">
	
	
	<!-- 로그인*회원정보가져오기 -->
	<select id="memberLoginCheckMethod" 
	  parameterType="mybatis.MemberVO" resultType="mybatis.MemberVO">
	  SELECT * FROM alouer_member WHERE memberId = #{memberId } AND pass=#{pass }
	</select>
</mapper>